<p id="notice"><%= notice %></p>

<h1>Listing Elems</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @elems.each do |elem| %>
      <tr>
        <td><%= link_to 'Show', elem %></td>
        <td><%= link_to 'Edit', edit_elem_path(elem) %></td>
        <td><%= link_to 'Destroy', elem, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Elem', new_elem_path %>
